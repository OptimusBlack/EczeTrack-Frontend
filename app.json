{
  "expo": {
    "name": "EczeTrack",
    "slug": "eczetrack",
    "scheme": "eczetrack",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wederm.eczetrack",
      "buildNumber": "1.0.0"

    },
    "android": {
      "package": "com.wederm.eczetrack",
      "versionCode": 1,
      "useNextNotificationsApi": true
    },
    "description": "",
    "githubUrl": "https://github.com/alifarooqi/EczeTrack-Frontend"
  }
}
